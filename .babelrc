{
  "presets": [
    "@babel/env",
    [
      "@babel/react",
      {
        "throwIfNamespace": false
      }
    ],
    "@babel/flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-fbt",
    "babel-plugin-fbt-runtime"
  ]
}
